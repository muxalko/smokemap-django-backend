{
  "version": 2,
  "builds": [
    {
      "src": "smokemap/wsgi.py",
      "use": "vercel-python-gis@1.0.0",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" },
      "buildCommand": "build_files.sh"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "smokemap/wsgi.py"
    }
  ]
}
